@model List<SWGDealer.Models.DealerModels.VehicleMake>

@{
    ViewBag.Title = "Makes";
}

<h2>Makes</h2>

<div class="row">
	<div class="col-md-12">
		@Html.ActionLink("Add Make", "AddMake", "Admin", new { @class = "btn btn-primary" })
		<table class="table table-bordered table-striped">
			<thead>
				<tr>
					<th>Model </th>
					<th>Date Added</th>
					<th>User</th>
				</tr>
			</thead>
			<tbody>
				@foreach (var car in Model)
				{
					<tr>
						<td>@car.VehicleMakeName</td>
						<td>@car.DateAdded</td>
						<td>@car.User.UserName</td>
					</tr>
				}
			</tbody>
		</table>
	</div>
</div>